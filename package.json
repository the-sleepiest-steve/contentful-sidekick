  "bugs": {
    "url": "hthttps://github.com/the-sleepiest-steve/contentful-sidekick/issues"
  },
  "homepage": "https://github.com/the-sleepiest-steve/contentful-sidekick#readme",
  "dependencies": {
    "common-tags": "^1.8.0",
    "contentful-management": "^5.4.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "webpack": "^4.28.4",
    "webpack-merge": "^4.2.1"
